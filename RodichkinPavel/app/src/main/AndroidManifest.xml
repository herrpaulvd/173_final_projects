<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="SomeApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        tools:targetApi="31">
        <activity
            android:name=".fscreen_activity"
            android:exported="true"
            android:label="Вход"
            android:theme="@style/Theme.AppCompat.Light">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".register_activity"
            android:label="Регистрация"
            android:theme="@style/Theme.AppCompat.Light" />
        <activity android:name=".showHs_activity"
            android:label="Таблица лидеров"
            android:theme="@style/Theme.AppCompat.Light" />
        <activity android:name=".choose_activity"
            android:label="Выберите дальнешее действие"
            android:theme="@style/Theme.AppCompat.Light" />
        <activity android:name=".game_activity"
            android:label="Учимся считать"
            android:theme="@style/Theme.AppCompat.Light" />
    </application>

</manifest>